# 공공데이터포털 API 키 (https://www.data.go.kr)
DATA_GO_KR_API_KEY=your_data_go_kr_api_key

# 청약홈 인증 오류 시에만 추가 (기본은 DATA_GO_KR_API_KEY 재사용)
# ODCLOUD_SERVICE_KEY=your_odcloud_service_key

# 온비드 인증 오류 시에만 추가
# ONBID_API_KEY=your_onbid_api_key

# PostgreSQL
DATABASE_URL=postgresql://jibscan:jibscan@localhost:5432/jibscan

